{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Go: Run",
            "type": "shell",
            "command": "go",
            "args": [
                "run",
                "."
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$go"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}
